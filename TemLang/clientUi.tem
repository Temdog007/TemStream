enum ClientCommand {
    PrintName
    GetClients
    ConnectToStream
    DisconnectFromStream
    StartStreaming
    SendStreamData
    StopStreaming
    Quit
}

struct Rect {
    f32 x
    f32 y
    f32 w
    f32 h
}

resource StreamDisplay {
    Rect rect
    null texture
    Guid id
} r {
    inlineC "
        SDL_DestroyTexture(r.texture);
    "
}

inlineC "MAKE_DEFAULT_LIST(StreamDisplay);"